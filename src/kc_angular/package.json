{
  "name": "kc_angular",
  "version": "0.6.1",
  "description": "This package contains the (core) Angular portion of the Knowledge web-app.",
  "author": "Rob Royce <robertris2@gmail.com> (https://robroyce.dev)",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build main --configuration production --base-href ./",
    "build-dev": "ng build main --configuration development --base-href ./",
    "build-main": "ng build main --configuration production --base-href ./",
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules .angular",
    "start": "echo 'Run this command from the top level directory to start Electron and load the Angular modules.'",
    "test": "ng test",
    "watch": "ng build --configuration production --base-href ./ --watch",
    "watch-dev": "ng build --configuration development --base-href ./ --watch",
    "watch-main": "ng build main --configuration production --base-href ./ --watch",
    "watch-main-dev": "ng build main --configuration development --base-href ./ --watch",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "~14.1.0",
    "@angular/compiler": "~14.1.0",
    "@angular/core": "~14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "~14.1.0",
    "@angular/youtube-player": "^14.1.0",
    "@fullcalendar/angular": "^5.11.2",
    "@fullcalendar/daygrid": "^5.11.2",
    "@fullcalendar/interaction": "^5.11.2",
    "@fullcalendar/list": "^5.11.2",
    "@fullcalendar/timegrid": "^5.11.2",
    "chart.js": "^3.8.0",
    "chokidar": "^3.5.2",
    "eslint": "^8.5.0",
    "fuse.js": "^6.6.2",
    "primeflex": "^3.2.1",
    "primeicons": "^6.0.1",
    "primeng": "^14.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/node": "^16.11.46",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "ng-packagr": "^14.1.0",
    "typescript": "^4.7.4"
  }
